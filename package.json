{
  "name": "babylon",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "server": "node ./bin/www",
    "start": "npm run babel-node && npm run server",
    "babel-node": "babel src --out-dir public --presets=@babel/env",
    "start:dev": "nodemon --watch src --exec npm run babel-node && npm run start"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "babylonjs": "^4.2.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "hbs": "~4.0.4",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.12",
    "@babel/plugin-transform-arrow-functions": "^7.13.0",
    "@babel/preset-env": "^7.13.12"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
